{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "hardhat run",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/hardhat",
      "cwd": "${workspaceFolder}",
      "args": ["bridge-confirm-message", "--network", "matic", "--vaabytes", ""]
    }
  ]
}